<h1>Sign Up</h1>
<%= form_for @user, url: users_path do |f|%>
    <%= f.label :name%>
    <%=f.text_field :name%><br>

    <%= f.label :password%>
    <%=f.text_field :password%><br>
    <%= f.label :password_confirmation%>
    <%=f.text_field :password_confirmation%><br>
    
    <%=f.submit "create"%>
<%end%>